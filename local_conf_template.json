{
  "gateway_conf": {
    "gateway_ID": "${GATEWAY_EUI}",
    "servers": [
      {
        "server_address": "${TTN_GATEWAY_SERVER_URL}",
        "serv_port_up": ${TTN_GATEWAY_PORT_UP},
        "serv_port_down": ${TTN_GATEWAY_PORT_DOWN},
        "serv_enabled": true
      }
    ],
    "ref_latitude": ${GATEWAY_LAT},
    "ref_longitude": ${GATEWAY_LON},
    "ref_altitude": ${GATEWAY_ALT},
    "contact_email": "${GATEWAY_EMAIL}",
    "description": "${GATEWAY_NAME}"
  }
}
